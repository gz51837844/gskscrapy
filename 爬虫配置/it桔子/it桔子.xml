<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" >
<xsl:template match="/">
<company>
<xsl:apply-templates select="//*[@class='line-title' and count(.//*[@class='seo-important-title']/text()[position()=1])>0]" mode="company"/>
</company>
</xsl:template>


<xsl:template match="//*[@class='line-title' and count(.//*[@class='seo-important-title']/text()[position()=1])>0]" mode="company">
<item>
<企业名称>
<xsl:value-of select="normalize-space(*//*[@class='seo-important-title']/text()[position()=1])"/>
<xsl:value-of select="normalize-space(*[@class='seo-important-title']/text()[position()=1])"/>
<xsl:if test="normalize-space(@class='seo-important-title')">
<xsl:value-of select="normalize-space(text()[position()=1])"/>
</xsl:if>
</企业名称>
<行业>
<xsl:value-of select="following-sibling::div[position()=2]/span[position()=1]/a[position()=2]"/>
</行业>
<省区>
<xsl:value-of select="following-sibling::div[position()=2]/span[position()=2]/a[position()=1]"/>
</省区>
<市区>
<xsl:value-of select="following-sibling::div[position()=2]/span[position()=2]/a[position()=2]"/>
</市区>
<网址>
<xsl:value-of select="following-sibling::div[position()=3]/span/a[position()=3]"/>
</网址>
</item>
</xsl:template>
</xsl:stylesheet>